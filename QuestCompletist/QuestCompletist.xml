      <CheckButton name="$parentHuman" parentKey="Human" checked="true" inherits="InterfaceOptionsCheckButtonTemplate">
        <Anchors>
          <Anchor point="TOPLEFT" relativePoint="TOPRIGHT" relativeTo="$parentAlliance">
            <Offset x="110" y="0"/>
