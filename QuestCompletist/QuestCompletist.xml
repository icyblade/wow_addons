      <OnLoad>
        self:RegisterForDrag("LeftButton")
        _G["qcQuestBuilderAllianceText"]:SetText("Alliance")
        _G["qcQuestBuilderHordeText"]:SetText("Horde")
        _G["qcQuestBuilderHumanText"]:SetText("Human")
        _G["qcQuestBuilderDwarfText"]:SetText("Dwarf")
        _G["qcQuestBuilderNightelfText"]:SetText("Nightelf")
        _G["qcQuestBuilderGnomeText"]:SetText("Gnome")
        _G["qcQuestBuilderDraeneiText"]:SetText("Draenei")
        _G["qcQuestBuilderWorgenText"]:SetText("Worgen")
      </OnLoad>
      <OnDragStart>self:StartMoving()</OnDragStart>
      <OnDragStop>self:StopMovingOrSizing()</OnDragStop>
