      <CheckButton name="$parentNightelf" parentKey="Nightelf" checked="true" inherits="InterfaceOptionsCheckButtonTemplate">
        <Anchors>
          <Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" relativeTo="$parentDwarf"/>
        </Anchors>
      </CheckButton>
      <CheckButton name="$parentGnome" parentKey="Gnome" checked="true" inherits="InterfaceOptionsCheckButtonTemplate">
        <Anchors>
