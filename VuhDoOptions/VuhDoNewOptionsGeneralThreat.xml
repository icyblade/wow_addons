			<Frame name="$parentAggroPanel" inherits="VuhDoPanelTemplate">
				<Size>
					<AbsDimension x="280" y="130" />
				</Size>
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="125" y="-180" />
						</Offset>
					</Anchor>
				</Anchors>
				<Frames>

					<Frame name="$parentRefreshSlider" inherits="VuhDoHSliderTemplate">
