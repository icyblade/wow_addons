							<Anchor point="TOPLEFT">
								<Offset>
									<AbsDimension x="13" y="-40" />
								</Offset>
							</Anchor>
						</Anchors>
						<Scripts>
							<OnLoad>
								VUHDO_setComboModel(self, "VUHDO_PANEL_SETUP.#PNUM#.PANEL_COLOR.barTexture", VUHDO_STATUS_BARS);
								VUHDO_lnfSetTooltip(self, VUHDO_I18N_TT.K076);
